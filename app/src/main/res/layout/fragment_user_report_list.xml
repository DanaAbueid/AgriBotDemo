<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_gray"
    tools:context=".screens.UserMyProfileFragment"
    >


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/light_gray">

        <ImageView
            android:id="@+id/imageView44"
            android:layout_width="366dp"
            android:layout_height="127dp"
            android:layout_marginStart="22dp"
            android:layout_marginTop="206dp"
            android:layout_marginEnd="23dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rounded_corner_background2" />

        <ImageView
            android:id="@+id/imageView45"
            android:layout_width="162dp"
            android:layout_height="116dp"
            android:adjustViewBounds="true"
            android:cropToPadding="true"
            app:layout_constraintBottom_toBottomOf="@+id/imageView44"
            app:layout_constraintEnd_toEndOf="@+id/imageView44"
            app:layout_constraintTop_toTopOf="@+id/imageView44"
            app:srcCompat="@drawable/analysis_amico" />

        <Button
            android:id="@+id/button10"
            style="@style/genericButton"
            android:layout_width="193dp"
            android:layout_height="48dp"
            android:layout_marginStart="16dp"
            android:backgroundTint="@color/green3"
            android:text="Report "
            app:layout_constraintBottom_toBottomOf="@+id/imageView44"
            app:layout_constraintStart_toStartOf="@+id/imageView44"
            app:layout_constraintTop_toTopOf="@+id/imageView44"
            app:layout_constraintVertical_bias="0.822" />

        <ImageView
            android:id="@+id/imageView40"
            android:layout_width="416dp"
            android:layout_height="166dp"
            android:src="@drawable/green7_background"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_condition_label8"
            android:layout_width="169dp"
            android:layout_height="58dp"
            android:layout_marginStart="32dp"
            android:backgroundTint="@color/green3"
            android:fontFamily="@font/bree_serif"
            android:text="Do you have any problem?"
            android:textAlignment="viewStart"
            android:textColor="@color/green3"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView44"
            app:layout_constraintStart_toStartOf="@+id/imageView44"
            app:layout_constraintTop_toTopOf="@+id/imageView44"
            app:layout_constraintVertical_bias="0.101" />

        <include
            android:id="@+id/view_toolbar"
            layout="@layout/view_toolbar"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_welcome3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:layout_marginTop="79dp"
            android:layout_marginEnd="127dp"
            android:fontFamily="@font/bree_serif"
            android:text="Reports List"
            android:textAlignment="viewStart"
            android:textColor="@color/light_gray"
            android:textSize="48sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_welcome4"
            android:layout_width="233dp"
            android:layout_height="53dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="162dp"
            android:layout_marginBottom="9dp"
            android:fontFamily="@font/bree_serif"
            android:text="My Reports"
            android:textAlignment="viewStart"
            android:textColor="@color/dark_green1"
            android:textSize="34sp"
            app:layout_constraintBottom_toTopOf="@+id/rv_user_report_list"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_user_report_list"
            android:layout_width="352dp"
            android:layout_height="434dp"
            android:layout_marginTop="450dp"
            android:paddingHorizontal="@dimen/_10sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </androidx.recyclerview.widget.RecyclerView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
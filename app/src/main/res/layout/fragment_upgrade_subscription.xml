<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_gray"
    tools:context=".screens.UpgradeSubscriptionFragment">

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="415dp"
        android:layout_height="165dp"
        android:src="@drawable/green7_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/view_toolbar"
        layout="@layout/view_toolbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_soil2"
        android:layout_width="346dp"
        android:layout_height="69dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="49dp"
        android:fontFamily="@font/bree_serif"
        android:text="Subscriptions"
        android:textAlignment="viewStart"
        android:textColor="@color/light_gray"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/cv_general_info2"
        android:layout_width="367dp"
        android:layout_height="278dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="34dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_welcome9"
        app:srcCompat="@drawable/rounded_corner_background2" />

    <TextView
        android:id="@+id/tv_welcome9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="161dp"
        android:fontFamily="@font/bree_serif"
        android:text="Subscription Information "
        android:textAlignment="viewStart"
        android:textColor="@color/dark_green1"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/subs_headline"
        android:layout_width="89dp"
        android:layout_height="40dp"
        android:layout_marginBottom="25dp"
        android:gravity="center|left"
        android:text="Subscription ID"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/starting_headline"
        app:layout_constraintEnd_toStartOf="@+id/tv_subs_get"
        app:layout_constraintStart_toStartOf="@+id/cv_general_info2" />

    <TextView
        android:id="@+id/starting_headline"
        android:layout_width="89dp"
        android:layout_height="40dp"
        android:layout_marginBottom="25dp"
        android:gravity="center|left"
        android:text="Starting Date "
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/ending_headline"
        app:layout_constraintEnd_toStartOf="@+id/tv_start_get"
        app:layout_constraintStart_toStartOf="@+id/cv_general_info2" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/tv_start_get"
        style="@style/genericEditText"
        android:layout_width="258dp"
        android:layout_height="48dp"
        android:layout_marginStart="105dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="20dp"
        android:hint="Starting Date "
        app:boxBackgroundColor="@color/cardview_shadow_start_color"
        app:layout_constraintBottom_toTopOf="@+id/tv_end_get"
        app:layout_constraintEnd_toEndOf="@+id/cv_general_info2"
        app:layout_constraintStart_toStartOf="@+id/cv_general_info2">

        <com.google.android.material.textfield.TextInputEditText
            style="@style/genericMaterialTextInputEditText"
            android:inputType="textWebEditText" />

    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/ending_headline"
        android:layout_width="89dp"
        android:layout_height="40dp"
        android:layout_marginBottom="25dp"
        android:gravity="center|left"
        android:text="Ending  Date "
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/price_headline"
        app:layout_constraintEnd_toStartOf="@+id/tv_end_get"
        app:layout_constraintStart_toStartOf="@+id/cv_general_info2" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/tv_end_get"
        style="@style/genericEditText"
        android:layout_width="258dp"
        android:layout_height="48dp"
        android:layout_marginStart="105dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="20dp"
        android:hint="Ending Date "
        app:boxBackgroundColor="@color/cardview_shadow_start_color"
        app:layout_constraintBottom_toTopOf="@+id/tv_price_get"
        app:layout_constraintEnd_toEndOf="@+id/cv_general_info2"
        app:layout_constraintStart_toStartOf="@+id/cv_general_info2">

        <com.google.android.material.textfield.TextInputEditText
            style="@style/genericMaterialTextInputEditText"
            android:inputType="textWebEditText" />

    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/price_headline"
        android:layout_width="89dp"
        android:layout_height="40dp"
        android:layout_marginBottom="21dp"
        android:gravity="center|left"
        android:text="Price"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/cv_general_info2"
        app:layout_constraintEnd_toStartOf="@+id/tv_price_get"
        app:layout_constraintStart_toStartOf="@+id/cv_general_info2" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/tv_price_get"
        style="@style/genericEditText"
        android:layout_width="258dp"
        android:layout_height="48dp"
        android:layout_marginStart="105dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="12dp"
        android:hint="Price"
        app:boxBackgroundColor="@color/cardview_shadow_start_color"
        app:layout_constraintBottom_toBottomOf="@+id/cv_general_info2"
        app:layout_constraintEnd_toEndOf="@+id/cv_general_info2"
        app:layout_constraintStart_toStartOf="@+id/cv_general_info2">

        <com.google.android.material.textfield.TextInputEditText
            style="@style/genericMaterialTextInputEditText"
            android:layout_width="224dp"
            android:layout_height="match_parent"
            android:inputType="number"
            tools:ignore="Deprecated" />

    </com.google.android.material.textfield.TextInputLayout>

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="367dp"
        android:layout_height="116dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cv_general_info2"
        app:srcCompat="@drawable/rounded_corner_background2" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/tv_subs_get"
        style="@style/genericEditText"
        android:layout_width="258dp"
        android:layout_height="48dp"
        android:layout_marginStart="105dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="4dp"
        android:hint="Subscription ID"
        app:boxBackgroundColor="@color/cardview_shadow_start_color"
        app:layout_constraintBottom_toTopOf="@+id/tv_start_get"
        app:layout_constraintEnd_toEndOf="@+id/cv_general_info2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/cv_general_info2">

        <com.google.android.material.textfield.TextInputEditText
            style="@style/genericMaterialTextInputEditText"
            android:inputType="textWebEditText"
            tools:layout_editor_absoluteX="156dp"
            tools:layout_editor_absoluteY="588dp" />

    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/button"
        style="@style/genericButton"
        android:layout_width="193dp"
        android:layout_height="42dp"
        android:layout_marginStart="20dp"
        android:backgroundTint="@color/green3"
        android:text="Upgrade Now!"
        app:layout_constraintBottom_toBottomOf="@+id/imageView20"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20"
        app:layout_constraintVertical_bias="0.864" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="143dp"
        android:layout_height="88dp"
        android:layout_marginEnd="4dp"
        android:adjustViewBounds="true"
        android:cropToPadding="true"
        app:layout_constraintBottom_toBottomOf="@+id/imageView20"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20"
        app:layout_constraintVertical_bias="0.607"
        app:srcCompat="@drawable/consulting_pana" />

    <TextView
        android:id="@+id/tv_condition_label3"
        android:layout_width="188dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@color/green3"
        android:fontFamily="@font/bree_serif"
        android:text="Want to change your subscription?"
        android:textAlignment="viewStart"
        android:textColor="@color/green3"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView20"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20"
        app:layout_constraintVertical_bias="0.527" />

</androidx.constraintlayout.widget.ConstraintLayout>